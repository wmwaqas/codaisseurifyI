<h1>New Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :album %>
  <%= f.number_field :album %>

  <%= f.label :year %>
  <%= f.text_field :year %>

  <%= f.label :rating %>
  <%= f.text_field :rating %>

  <%= f.submit "Create" %>
<% end %>
