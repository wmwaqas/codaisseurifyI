<%= form_for @song do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :album %>
  <%= f.text_field :album %>

  <%= f.label :year %>
  <%= f.number_field :year %>

  <%= f.label :rating %>
  <%= f.number_field :rating %>

  <%= f.submit (@song.new_record? ? "Create" : "Update") %>

<% end %>
